{
  "name": "redux-reqseq",
  "version": "0.1.1",
  "homepage": "https://github.com/openlattice/redux-reqseq",
  "description": "JavaScript library for request sequence lifecycles in redux",
  "author": {
    "name": "OpenLattice, Inc.",
    "email": "support@openlattice.com",
    "url": "https://openlattice.com"
  },
  "contributors": [
    {
      "name": "Hristo Oskov",
      "email": "hristo@openlattice.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openlattice/redux-reqseq.git"
  },
  "bugs": {
    "url": "https://github.com/openlattice/redux-reqseq/issues"
  },
  "license": "Apache-2.0",
  "main": "build/reqseq.js",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-class-properties": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "better-npm-run": "0.1.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.7.0",
    "flow-bin": "^0.74.0",
    "jest": "^22.1.2",
    "moment": "^2.20.0",
    "npm-run-all": "^4.0.0",
    "rimraf": "2.6.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.1.0"
  },
  "scripts": {
    "build": "better-npm-run build",
    "build:min": "better-npm-run build:min",
    "clean": "rimraf build/*",
    "flow": "flow check",
    "lib": "better-npm-run webpack-dev-server",
    "lint": "eslint --config .eslintrc src",
    "release:major": "npm version major -m \"releasing v%s\" && npm publish",
    "release:minor": "npm version minor -m \"releasing v%s\" && npm publish",
    "release:patch": "npm version patch -m \"releasing v%s\" && npm publish",
    "preversion": "npm-run-all lint test",
    "version": "npm-run-all clean --parallel build build:min && git add -A package.json build/",
    "postversion": "git push && git push --tags",
    "test": "jest --config config/jest/jest.config.js",
    "test:watch": "jest --config config/jest/jest.config.js --watch"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config config/webpack/webpack.config.babel.js",
      "env": {
        "BUILD": "production",
        "MINIMIZE": "false",
        "TARGET_ENV": "web"
      }
    },
    "build:min": {
      "command": "webpack --config config/webpack/webpack.config.babel.js",
      "env": {
        "BUILD": "production",
        "MINIMIZE": "true",
        "TARGET_ENV": "web"
      }
    },
    "webpack-dev-server": {
      "command": "webpack-dev-server --config config/webpack/webpack.config.babel.js --port 9000 --content-base build/",
      "env": {
        "BUILD": "development"
      }
    }
  }
}
